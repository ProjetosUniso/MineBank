<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadastroEnederecoActivity">

    <ImageView
        android:id="@+id/background"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/loginbg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="206dp"
        android:layout_height="192dp"
        android:adjustViewBounds="true"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/background" />

    <TextView
        android:id="@+id/textViewCadastro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/minecraft"
        android:gravity="center"
        android:text="@string/cadastrartext"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo" />

    <TextView
        android:id="@+id/textViewCEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/minecraft"
        android:text="@string/cep"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextCEP"
        app:layout_constraintStart_toStartOf="@+id/editTextCEP"
        app:layout_constraintTop_toBottomOf="@+id/textViewCadastro" />

    <EditText
        android:id="@+id/editTextCEP"
        android:layout_width="247dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:autofillHints=""
        android:background="#000000"
        android:ems="10"
        android:fontFamily="@font/minecraft"
        android:inputType="number"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCEP" />

    <TextView
        android:id="@+id/textENDERECO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/minecraft"
        android:text="@string/endereco"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextENDERECO"
        app:layout_constraintStart_toStartOf="@+id/editTextENDERECO"
        app:layout_constraintTop_toBottomOf="@+id/editTextCEP" />

    <EditText
        android:id="@+id/editTextENDERECO"
        android:layout_width="247dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:autofillHints=""
        android:background="#656565"
        android:ems="10"
        android:fontFamily="@font/minecraft"
        android:inputType="text"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:focusable="false"
        android:longClickable="false"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textENDERECO" />

    <TextView
        android:id="@+id/textNUMERO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/minecraft"
        android:text="@string/numeroendereco"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextNUMERO"
        app:layout_constraintStart_toStartOf="@+id/editTextNUMERO"
        app:layout_constraintTop_toBottomOf="@+id/editTextENDERECO" />

    <EditText
        android:id="@+id/editTextNUMERO"
        android:layout_width="247dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:autofillHints=""
        android:background="#000000"
        android:ems="10"
        android:fontFamily="@font/minecraft"
        android:inputType="number"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNUMERO" />

    <TextView
        android:id="@+id/textCIDADE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/minecraft"
        android:text="@string/cidade"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextCIDADE"
        app:layout_constraintStart_toStartOf="@+id/editTextCIDADE"
        app:layout_constraintTop_toBottomOf="@+id/editTextNUMERO" />

    <EditText
        android:id="@+id/editTextCIDADE"
        android:layout_width="180dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:autofillHints=""
        android:background="#656565"
        android:ems="10"
        android:fontFamily="@font/minecraft"
        android:inputType="text"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:focusable="false"
        android:longClickable="false"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/editTextNUMERO"
        app:layout_constraintTop_toBottomOf="@+id/textCIDADE" />

    <TextView
        android:id="@+id/textESTADO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/minecraft"
        android:text="@string/estado"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextESTADO"
        app:layout_constraintStart_toStartOf="@+id/editTextESTADO"
        app:layout_constraintTop_toTopOf="@+id/textCIDADE" />

    <EditText
        android:id="@+id/editTextESTADO"
        android:layout_width="47dp"
        android:layout_height="36dp"
        android:autofillHints=""
        android:background="#656565"
        android:ems="10"
        android:fontFamily="@font/minecraft"
        android:inputType="text"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:maxLength="2"
        android:focusable="false"
        android:longClickable="false"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintEnd_toEndOf="@id/editTextNUMERO"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editTextCIDADE"
        app:layout_constraintTop_toTopOf="@id/editTextCIDADE" />

    <Button
        android:id="@+id/continuarButton"
        android:layout_width="127dp"
        android:layout_height="37dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:background="@drawable/button"
        android:fontFamily="@font/minecraft"
        android:text="@string/continuarbutton"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/voltarButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toEndOf="@+id/voltarButton" />

    <Button
        android:id="@+id/voltarButton"
        android:layout_width="127dp"
        android:layout_height="37dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/button"
        android:fontFamily="@font/minecraft"
        android:text="@string/voltarbutton"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextESTADO" />

</androidx.constraintlayout.widget.ConstraintLayout>